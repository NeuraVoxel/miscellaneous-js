{
  "name": "hellogrpc-backend",
  "version": "1.0.0",
  "description": "gRPC Node.js backend server example",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./proto --grpc_out=grpc_js:./proto --proto_path=../proto ../proto/*.proto"
  },
  "keywords": [
    "grpc",
    "node",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "@grpc/proto-loader": "^0.7.0",
    "google-protobuf": "^3.21.0"
  },
  "devDependencies": {
    "grpc-tools": "^1.12.0",
    "nodemon": "^2.0.20"
  }
}